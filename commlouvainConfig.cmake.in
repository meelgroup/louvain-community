# Config file for the @EXPORT_TYPE@ cryptominisat Package
# It defines the following variables
#  COMMLOUVAIN_INCLUDE_DIRS - include directories for comm_louvain
#  COMMLOUVAIN_LIBRARIES    - libraries to link against
#  COMMLOUVAIN_EXECUTABLE   - the community-louvain executable

# Compute paths
get_filename_component(COMMLOUVAIN_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(COMMLOUVAIN_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")

# Our library dependencies (contains definitions for IMPORTED targets)
include("${COMMLOUVAIN_CMAKE_DIR}/@COMMLOUVAIN_TARGETS_FILENAME@")

# These are IMPORTED targets created by @COMMLOUVAIN_TARGETS_FILENAME@
set(COMMLOUVAIN_VERSION_MAJOR @PROJECT_VERSION_MAJOR@)
set(COMMLOUVAIN_VERSION_MINOR @PROJECT_VERSION_MINOR@)
set(COMMLOUVAIN_LIBRARIES louvain_communities)
